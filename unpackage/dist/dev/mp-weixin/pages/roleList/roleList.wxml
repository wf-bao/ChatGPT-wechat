<view class="container"><view class="select_btn"><button type="primary" plain="{{numId==0?false:true}}" size="mini" data-event-opts="{{[['tap',[['changeType',[0]]]]]}}" bindtap="__e">全部</button><block wx:for="{{dateInfo}}" wx:for-item="item" wx:for-index="index" wx:key="index"><button type="primary" plain="{{item.id==numId?false:true}}" size="mini" data-event-opts="{{[['tap',[['changeType',['$0'],[[['dateInfo','',index,'id']]]]]]]}}" bindtap="__e">{{item.btnTitle}}</button></block></view><block wx:if="{{viewAllFlag}}"><view class="role_list"><view class="title"><text>{{dateInfo[numId-1].btnTitle}}</text></view><view class="care_list"><block wx:for="{{dateInfo[numId].careList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><uni-card vue-id="{{'5f99d0c0-1-'+index}}" title="{{item.careTitle}}" extra="复制" bind:__l="__l" vue-slots="{{['default']}}"><text class="uni-body">{{item.careContent}}</text><label data-event-opts="{{[['tap',[['copyMassage',['$0'],[[['dateInfo.'+numId+'.careList','',index,'careContent']]]]]]]}}" class="copy _span" bindtap="__e"></label></uni-card></block></view></view></block><block wx:else><view class="role_list select_all"><block wx:for="{{dateInfo}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view><text class="title">{{item.btnTitle}}</text><block wx:for="{{item.careList}}" wx:for-item="article" wx:for-index="i" wx:key="i"><uni-card vue-id="{{'5f99d0c0-2-'+index+'-'+i}}" title="{{article.careTitle}}" extra="复制" bind:__l="__l" vue-slots="{{['default']}}"><text class="uni-body">{{article.careContent}}</text><label data-event-opts="{{[['tap',[['copyMassage',['$0'],[[['dateInfo','',index],['careList','',i,'careContent']]]]]]]}}" class="copy _span" bindtap="__e"></label></uni-card></block></view></block></view></block></view>